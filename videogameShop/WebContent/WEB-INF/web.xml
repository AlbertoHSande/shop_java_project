<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>videogameShop</display-name>
  <welcome-file-list>
    <welcome-file>index</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>services.ServletIndex</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>s</servlet-name>
    <servlet-class>services.ServletProducto</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>s</servlet-name>
    <url-pattern>/servlet_producto</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>u</servlet-name>
    <servlet-class>services.ServletUsuario</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>u</servlet-name>
    <url-pattern>/servlet_usuario</url-pattern>
  </servlet-mapping>
</web-app>